<section class="page">
  <div class="centered-box">
    <h1>Create new user</h1>

    <form action="<%= users_url %>" method="post">
      <input
        type="hidden"
        name="authenticity_token"
        value="<%= form_authenticity_token %>">

      <label for="user_email">User Name</label>
      <input type="text"
        name="user[email]"
        id="user_email"
        value="<%= @user.email %>">
      <br>
      <br>
      <label for="user_password">Password</label>
      <input type="password"
        name="user[password]"
        id="user_password"
        value="<%= @user.password %>">
      <br>
      <br>
      <input type="submit"> 
    </form>
  </div>
</section>